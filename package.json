{
  "name": "rfid-utils",
  "private": false,
  "description": "A utility library for RFID operations in TypeScript.",
  "author": "liqingshan",
  "license": "MIT",
  "version": "0.0.11",
  "type": "module",
  "types": "./dist/rfid-utils.d.ts",
  "main": "./dist/rfid-utils.js",
  "files": [
    "./dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/rfid-utils.d.ts",
      "import": "./dist/rfid-utils.mjs",
      "require": "./dist/rfid-utils.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.0",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.21" 
  }
}
